{
  "version": 2,
  "functions": {
    "api/config.js": {
      "runtime": "nodejs18.x"
    },
    "api/farcaster-manifest.js": {
      "runtime": "nodejs18.x"
    },
    "api/test-neynar.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/api/farcaster-manifest"
    },
    {
      "src": "/.well-known/(.*)",
      "dest": "/.well-known/$1"
    },
    {
      "src": "/(.*\\.(js|css|html|ico|png|jpg|jpeg|gif|svg|json))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
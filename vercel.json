{
  "version": 2,
  "routes": [
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/api/farcaster-manifest"
    },
    {
      "src": "/.well-known/(.*)",
      "dest": "/.well-known/$1"
    },
    {
      "src": "/(.*\\.(js|css|html|ico|png|jpg|jpeg|gif|svg|json))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}